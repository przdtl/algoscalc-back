{
  "title": "Симплекс-метод",
  "description": "Алгоритм решения оптимизационной задачи линейного программирования путём перебора вершин выпуклого многогранника в многомерном пространстве",
  "parameters": [
    {
      "name": "tableau",
      "title": "Матрица коэффициентов",
      "description": "Матрица коэффициентов",
      "data_type": "FLOAT",
      "data_shape": "MATRIX",
      "default_value": [[0.0, -1.0, -1.0, 0.0, 0.0],
                        [24.0, 6.0, 4.0, 1.0, 0.0],
                        [6.0, 3.0, -2.0, 0.0, 1.0]]
    },
    {
      "name": "basic_var",
      "title": "Базисные переменные",
      "description": "Базисные переменные",
      "data_type": "FLOAT",
      "data_shape": "LIST",
      "default_value": [1.0, 2.0]
    }
  ],
  "outputs": [
    {
      "name": "optimal_solution",
      "title": "Оптимальное решение",
      "description": "Решение, при котором обеспечивается максимальное (минимальное) значение целевой функции",
      "data_type": "FLOAT",
      "data_shape": "LIST",
      "default_value": [0.0, 6.0, 0.0, 18.0]
    },
    {
      "name": "optimal_value",
      "title": "Оптимальное значение",
      "description": "Оптимальное значение",
      "data_type": "FLOAT",
      "data_shape": "SCALAR",
      "default_value": -6.0
    }
  ]
}
